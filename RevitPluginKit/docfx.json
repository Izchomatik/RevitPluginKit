{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "*.csproj"
          ],
          "cwd": ".",
          "exclude": [
            "**/obj/**",
            "**/bin/**",
            "_site/**"
          ]
        }
      ],
      "dest": "obj/api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml"
        ],
        "cwd": "obj"
      },
      {
        "files": [
          "api/*.md",
          "articles/**.md",
          "toc.yml",
          "*.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "resource": [
      {
        "src": "../docs",
        "files": [
          "images/**"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "globalMetadata": {
      "_appName": "RevitPluginKit",
      "_appTitle": "RevitPluginKit Documentation",
      "_appFooter": "RevitPluginKit - <a href= https://github.com/Izchomatik/RevitPluginKit >View on Github</a> <br/>Generated by <b>DocFX</b>",
      "_appFaviconPath": "../docs/images/RPKLogo.ico",
      "_enableSearch": true,
      "_disableAffix": true,
      "_disableSideFilter": true,
      "_gitContribute": {
        "repo": "https://github.com/Izchomatik/RevitPluginKit"
      },
      "_gitUrlPattern": "github"
    },
    "dest": "../docs",
    "template": [
      "default",
      "../docs/templates/discordfx"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}
