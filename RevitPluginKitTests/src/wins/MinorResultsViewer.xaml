<Window x:Class="RevitPluginKitTests.Wins.MinorResultsViewer"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Title="Results" SizeToContent="Height" Width="350" WindowStartupLocation="CenterScreen" ResizeMode="NoResize">
    <StackPanel>
        <TextBlock x:Name="wpfTitleBlock" FontWeight="DemiBold" TextWrapping="Wrap" Margin="10"></TextBlock>
        <TextBlock x:Name="wpfResultsBlock" TextWrapping="Wrap" Margin="10"></TextBlock>
        <Button Height="30" IsCancel="True" Margin="10">Close</Button>
    </StackPanel>
</Window>
